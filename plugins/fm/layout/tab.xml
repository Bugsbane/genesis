        <button id="newfld" text="New folder" iconfont="gen-folder-plus" />
        <button id="close" text="Close tab" iconfont="gen-cancel-circle" />
    </toolbar>

    <spacer height="10" />

    <simpleform id="files">
    <dt id="list" width="765">
        <dtr header="True">
            <dth width="1" />
